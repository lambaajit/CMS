@model DLCMS.Areas.WebsitePages.Controllers.WebsitePagesSearch

@Html.TextBoxFor(x => x.Name, new { @class = "form-control", style = "display:inline-block", placeholder = "Title" })
@Html.DropDownListFor(x => x.Department, (List<SelectListItem>)ViewBag.DepartmentList, "Department", new { @class = "form-control", style = "display:inline-block", placeholder = "Title" })
@Html.TextBoxFor(x => x.Filename, new { @class = "form-control", style = "display:inline-block", placeholder = "Filename" })
@Html.DropDownListFor(x => x.Company, (List<SelectListItem>)ViewBag.CompanyList, "Company", new { @class = "form-control", style = "display:inline-block", placeholder = "Title" })
@Html.DropDownListFor(model => model.VideoId, (List<SelectListItem>)ViewBag.VideoList, "Video", new { @class = "form-control",style = "display:inline-block" })

@{ Html.RenderPartial("~/Views/GenericBase/ColumnsToDisplay.cshtml", new dlwebclasses.AllProperties() { ClassProperties = Model.ClassProperties, NumberOfRecordsPerPages = Model.NumberOfRecordsPerPages }); }

<script type="text/javascript">

    $(document).ready(function () {
        CKEDITOR.replace('Text', {
            height: 400
        });
    });

    var WebsitePages = {
        InitialiseForm: function (data) {
            CKEDITOR.replace('Text', {
                height: 400
            });
        }
    };
</script>
